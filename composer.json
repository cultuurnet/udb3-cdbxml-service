{
    "name": "cultuurnet/udb3-cbxml-service",
    "description": "Brief description of this Silex application",
    "type": "project",
    "license": "Apache-2.0",
    "authors": [],
    "require": {
        "silex/silex": "~1.3",
        "deralex/yaml-config-service-provider": "~1.0",
        "doctrine/cache": "~1.3",
        "knplabs/console-service-provider": "~1.0",
        "monolog/monolog": "~1.11",
        "predis/predis": "~1.0",
        "cultuurnet/broadway-amqp": "~1.0",
        "cultuurnet/deserializer": "~1.0",
        "cultuurnet/geocoding": "~0.1",
        "cultuurnet/silex-amqp": "~1.0",
        "cultuurnet/udb3": "dev-feature/III-404 as 0.1",
        "cultuurnet/cdb": "~2.2",
        "cultuurnet/udb2-domain-events": "~1.0",
        "crell/api-problem": "^1.7",
        "doctrine/dbal": "^2.4",
        "doctrine/migrations": "~1.0",
        "willdurand/geocoder": "~2.8",
        "commerceguys/intl": "^0.7.1",
        "guzzle/guzzle": "^3.9",
        "league/uri": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "escapestudios/symfony2-coding-standard": "~2.9",
        "squizlabs/php_codesniffer": "~2.5.1",
        "phing/phing": "~2.14",
        "satooshi/php-coveralls": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "CultuurNet\\UDB3\\CdbXmlService\\": [
                "src/",
                "app/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CultuurNet\\UDB3\\CdbXmlService\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "config": {
        "preferred-install": {
            "cultuurnet/*": "source",
            "2dotstwice/*": "source",
            "*": "dist"
        },
        "platform": {
            "php": "5.6.27"
        }
    }
}
